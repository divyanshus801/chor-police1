{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" },
      { "src": "../../routes/route.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" },
      { "src": "/routes-api/(.*)", "dest": "/routes/route.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  