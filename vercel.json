{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" },
      { "src": "./router.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" },
      { "src": "/router-api/(.*)", "dest": "/router.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  